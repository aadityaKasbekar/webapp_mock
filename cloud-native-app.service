[Unit]
Description=cloud-native-app
After=syslog.target

[Service]
User=ec2-user
EnvironmentFile=/etc/environment
ExecStart=/usr/bin/java -jar /tmp/movieRetirvalWebApp-0.0.1-SNAPSHOT.jar
StandardOutput=file:/home/ec2-user/logs/cloudNativeApp_csye6225.log
StandardError=file:/home/ec2-user/logs/cloudNativeApp_csye6225
SuccessExitStatus=143

[Install]
WantedBy=multi-user.target